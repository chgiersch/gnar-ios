      "frequency": "daily",
      "abbreviation": "SS"
    },
    {
      "id": "tram-announcement",
      "name": "Tram Announcement",
      "descriptionText": "Get the Tram Operator to announce to a full tram load, 'And also today (your name) gets 2,000 GNAR extra credit points.' Only one player per tram ride.",
      "points": 2000,
      "frequency": "daily",
      "abbreviation": "TA"
    },
    {
      "id": "ten-bucks",
      "name": "Ten Bucks",
      "descriptionText": "Give out at least 5 Red Bulls in the KT line before the lift opens. If sponsored by Red Bull, must also give out $2 with each can.",
      "points": 1500,
      "frequency": "daily",
      "abbreviation": "TB"
    },
    {
      "id": "blues-diet",
      "name": "Blue\u2019s Diet",
      "descriptionText": "Eat and drink exactly what Blue does for one whole week. (Weekly / Monthly games)",
